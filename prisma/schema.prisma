generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum UserRole {
  ADMIN
  USER
}

model User {
  id         String   @id @unique
  name       String
  email      String   @unique
  password   String
  role       UserRole
  created_at BigInt
  updated_at BigInt?

  @@map("users")
}
